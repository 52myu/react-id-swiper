    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-jest": "22.2.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "babel-preset-es2015": "^6.24.0",
